#include<iostream>
using namespace std;
int main()
{        // lab 7 Task 3
    int A[7] , count=0;
    cout<<"Enter the seven integers\n ";
    for(int i=0; i<7; i++){
    cout<<"Enter the number :"<<i+1<<"  :";
    cin>>A[7];
    cout<<endl;
    if(A[7]%5==0)
    count++;
    }
    cout<<"\nThere are :"<<count <<" multiples of 5\n"<<endl;
    
    return 0;
}
